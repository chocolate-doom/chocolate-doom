				<Option object_output="..\obj\rel\" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="Debug;Release;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-D_WIN32" />
			<Add directory="." />
			<Add directory="..\src" />
			<Add directory="..\textscreen" />
		</Compiler>
		<Linker>
			<Add option="-ltextscreen -lmingw32 -lSDLmain -lSDL -mwindows" />
